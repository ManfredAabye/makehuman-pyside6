	"influences": [
		{ "name": "phenotype-gender-age", "comp": [ "phenotype", "gender", "age" ] },
		{ "name": "gender-age-muscle-weight", "comp": [ "!universal", "gender", "age", "muscle", "weight" ] },
		{ "name": "gender-age-muscle-weight-prop", "comp": [ "gender", "age", "muscle", "weight", "proportion" ], "folder": "proportions" },
		{ "name": "gender-age-muscle-weight-height", "comp": [ "gender", "age", "muscle", "weight", "height" ], "folder": "height" },
		{ "name": "breastsize-breastfirmness", "comp": [ "gender", "age", "muscle", "weight", "cupsize", "firmness" ], "folder": "breast" }
	]
