		"proportion":{"values": ["uncommonproportions", "idealproportions"], "steps": [0.0, 1.0], "pattern": "macrodetails-proportions/BodyProportions" },
		"height":    {"values": ["minheight", "maxheight"], "steps": [0.0, 1.0], "pattern": "macrodetails-height/Height" },
		"cupsize":   {"values": ["mincup", "averagecup", "maxcup"], "steps": [0.0, 0.5, 1.0], "pattern": "breast/BreastSize" },
